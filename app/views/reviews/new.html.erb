<h1>New review</h1>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :rating, collection: (1..5) %>
  <%= f.input :content %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>

<p><%= link_to "Back to restaurants", restaurants_path, class: "btn btn-secondary btn-sm" %>
